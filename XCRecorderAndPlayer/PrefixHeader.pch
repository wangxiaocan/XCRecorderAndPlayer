#define POWER_TO_DB(power)  ({\
    float vlume = power + 160 - 50;\
    int dB = 0;\
    if (vlume < 0.f) {\
        dB = 0;\
    } else if (vlume < 40.f) {\
        dB = (int)(vlume * 0.875);\
    } else if (vlume < 100.f) {\
        dB = (int)(vlume - 15);\
    } else if (vlume < 110.f) {\
        dB = (int)(vlume * 2.5 - 165);\
    } else {\
        dB = 110;\
    }\
    dB;\
})

#endif





#endif /* PrefixHeader_pch */
